{
  "name": "@jakechampion/c-at-e-file-server-cli",
  "version": "1.5.56",
  "description": "Fastly File Server uploads files to Fastly for serving directly from within Compute@Edge applications. Upload any type of file: images, text, video etc and serve directly from Fastly. It is ideal for serving files built from a static site generator such as 11ty.",
  "type": "module",
  "scripts": {
    "update": "node ./update.js",
    "postinstall": "node ./post-install.js"
  },
  "devDependencies": {
    "@felipecrs/decompress-tarxz": "^4.0.0",
    "decompress": "^4.2.1",
    "decompress-unzip": "^4.0.1",
    "zx": "^7.0.8"
  },
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "c-at-e-file-server": "c-at-e-file-server"
  },
  "optionalDependencies": {
    "@fastly/c-at-e-file-server-cli-darwin-arm64": "1.5.56",
    "@fastly/c-at-e-file-server-cli-darwin-x64": "1.5.56",
    "@fastly/c-at-e-file-server-cli-linux-x64": "1.5.56",
    "@fastly/c-at-e-file-server-cli-linux-s390x": "1.5.56",
    "@fastly/c-at-e-file-server-cli-win32-x64": "1.5.56"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeChampion/c-at-e-file-server.git"
  },
  "bugs": {
    "url": "https://github.com/JakeChampion/c-at-e-file-server/issues"
  },
  "homepage": "https://github.com/JakeChampion/c-at-e-file-server#readme"
}
